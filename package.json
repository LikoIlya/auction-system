{
  "name": "auction-app",
  "version": "1.0.0",
  "description": "Minimalistic boilerplate to quick-start Node.js development in TypeScript.",
  "engineStrict": true,
  "engines": {
    "node": ">= 10.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^5.0.4",
    "@types/jest": "~22.2.3",
    "@types/joi": "^13.4.0",
    "@types/koa": "^2.0.46",
    "@types/koa-compose": "^3.2.2",
    "@types/koa-jwt": "^3.3.0",
    "@types/koa-router": "^7.0.30",
    "@types/moment-timezone": "^0.5.7",
    "@types/node": "~8.10.18",
    "@types/supertest": "^2.0.5",
    "jest": "~23.1.0",
    "nodemon": "^1.18.3",
    "rimraf": "~2.6.2",
    "ts-jest": "~22.4.6",
    "tslint": "~5.10.0",
    "tslint-config-prettier": "^1.14.0",
    "tsutils": "~2.27.1",
    "typescript": "~2.9.1"
  },
  "scripts": {
    "build": "tsc -p tsconfig.release.json",
    "watch": "tsc -w -p tsconfig.release.json",
    "lint": "tslint -t stylish --project \"tsconfig.json\"",
    "prestart": "npm run build",
    "serve": "serve static",
    "start": "concurrently --kill-others \"npm run start-backend\" \"npm run serve\"",
    "start-backend": "node build/src"
  },
  "author": "Lalkek",
  "dependencies": {
    "@koa/cors": "^2.2.2",
    "concurrently": "^4.1.0",
    "dotenv": "^6.0.0",
    "fs-extra": "^7.0.0",
    "joi": "^13.5.2",
    "jsonwebtoken": "^8.3.0",
    "koa": "^2.5.2",
    "koa-body": "^4.0.4",
    "koa-compose": "^4.1.0",
    "koa-cors": "0.0.16",
    "koa-docs": "^2.1.0",
    "koa-jwt": "^3.3.2",
    "koa-morgan": "^1.0.1",
    "koa-respond": "^1.0.1",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.21",
    "pg": "^7.4.3",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.12",
    "serve": "^10.1.1",
    "supertest": "^3.1.0",
    "typeorm": "^0.2.7",
    "winston": "^3.0.0"
  }
}
